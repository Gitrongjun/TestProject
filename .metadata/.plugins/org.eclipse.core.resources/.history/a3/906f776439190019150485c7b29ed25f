package OutputCSV;

import java.io.File;

public class Test {
	
	public static void main(String[] args) {
		File file = new File("D:/test/ljq.csv");
		if (file.isFile() && file.exists()) {
			boolean delete = file.delete();
			if (!delete) {
				System.out.println("删除失败， 请关闭文件后再删除");
			}
		}
		
		
	}

}
